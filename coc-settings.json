{
  // General
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "sass",
    "scss",
    "markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "graphql",
    "html",
    "json",
    "jsonc",
    "yaml",
    "python",
    "vim",
    "vue",
    "hbs"
  ],
  "coc.preferences.colorSupport": false,
  "coc.preferences.bracketEnterImprove": true,
  /*"cSpell.enabled": false,*/
  // Diagnostic Settings
  "diagnostic.enable": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "➤",
  "diagnostic.warningSign": "",
  "diagnostic.errorSign": "",
  "codeLens.enable": false,
  // ESlint settings
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.run": "onSave",
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "eslint.enable": false,
  "eslint.nodePath": null,
  // Coc-Explorer
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "H": "gotoParent"
  },
  // HTML
  "html.filetypes": [
    "html",
    "handlebars",
    "hbs",
    "htmldjango"
  ],
  "html.autoClosingTags": true,
  // Prettier
  /*"prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "prettier.disableSuccessMessage": true,
  "prettier.disableLanguages": [],*/
  // Python Config
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "python.venvPath": "~/.local/share/virtualenvs/",
  /*"python.pythonPath": "~/.local/share/virtualenvs/server-sZrT94H9/bin/python",*/
  "python.formatting.autopep8Args": [],
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "information"
  },
  "python.analysis.diagnosticMode": "openFilesOnly",
  // Yank
  "yank.highlight.duration": 200,
  "yank.list.maxsize": 50,
  "yank.limit": 5,
  // Snippet
  "snippets.extends": {
    "cpp": [
      "c"
    ],
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  }
}
